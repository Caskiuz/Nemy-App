{
  "name": "my-app",
  "main": "client/index.js",
  "version": "1.0.0",
  "scripts": {
    "expo:dev": "npx expo start --localhost",
    "server:dev": "tsx -r dotenv/config server/server.ts dotenv_config_path=.env.local",
    "server:prod": "set NODE_ENV=production && tsx server/server.ts",
    "jobs:dev": "tsx server/jobsRunner.ts",
    "audit:financial": "tsx -r dotenv/config server/testFinancialAudit.ts dotenv_config_path=.env.local",
    "fix:earnings": "tsx -r dotenv/config server/fixOrderEarnings.ts dotenv_config_path=.env.local",
    "db:reset-financial": "tsx -r dotenv/config server/resetFinancialData.ts dotenv_config_path=.env.local",
    "pm2:start": "pm2 start ecosystem.config.json",
    "pm2:stop": "pm2 stop ecosystem.config.json",
    "pm2:restart": "pm2 restart ecosystem.config.json",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "production:start": "npm run pm2:start",
    "production:stop": "npm run pm2:stop",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/server.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx server/migrate.ts",
    "test:e2e": "playwright test e2e.spec.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:install": "playwright install",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "security-audit": "npm audit && npm run lint",
    "build:apk:production": "eas build --platform android --profile production --local",
    "build:apk:preview": "eas build --platform android --profile preview --local"
  },
  "dependencies": {
    "@expo-google-fonts/nunito": "^0.4.2",
