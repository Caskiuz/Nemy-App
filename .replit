run = "npm run production:start"
entrypoint = "server/server-demo.ts"
modules = ["nodejs-20", "postgresql-16"]

[nix]
channel = "stable-23_11"
packages = ["mysql80"]

[deployment]
run = ["npx", "tsx", "server/server.ts"]
deploymentTarget = "autoscale"
build = ["npm", "run", "expo:static:build"]

[env]
NODE_ENV = "production"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start Backend"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start Frontend"

[[workflows.workflow]]
name = "Start Backend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "tsx server/server.ts"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Start Frontend"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run expo:dev"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8081
externalPort = 8081

[[ports]]
localPort = 8082
externalPort = 3000

[agent]
expertMode = true
integrations = ["javascript_gemini_ai_integrations:2.0.0"]
